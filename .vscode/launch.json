{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(g++.exe) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: g++.exe build active file",
      "miDebuggerPath": "gdb.exe",
      "logging": {
        "trace": true,
        "traceResponse": true
      },
      "debugServerPath": "",
      "filterStdout": false,
      "filterStderr": false,
      "sourceFileMap": {
        "/mnt/c": "C:\\"
      },
      "serverLaunchTimeout": 10000
    }
  ]
}